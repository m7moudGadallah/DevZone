# Set the environment variables for development, production and test here .e.g 'development', 'testing'
NODE_ENV=development
PORT=3000

# Databases URL Connections
DATABASE_URL_DEV=
DATABASE_URL_PROD=
DATABASE_URL_TEST=

# Set rate limit for requests by settnig the max number of requests and the window time in minutes
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MIN=60

# JWT Configs
JWT_SECRET=
JWT_EXPIRE_IN=30d

# Cookie Configs
# COOKIE expires in days
COOKIE_EXPIRES_IN=30